    function dateFormatChanger(dateInfo) {
      let date = dateInfo.split(' ')[0];
      let time = dateInfo.split(' ')[1];
      let dateDetail = date.split('-');
      let timeDetail = time.split(':');

      // ìš”ì¼ êµ¬í•˜ê¸° jsëŠ” ë¡œì»¬ì‹œê°„ì„ ê¸°ì¤€ìœ¼ë¡œ ìš”ì¼ì„ íŒë‹¨í•˜ê¸°ë•Œë¬¸ì— í‘œì¤€ì‹œë¡œ ë³´ì •ì´ í•„ìš”í•¨
      let week = ['ì¼', 'ì›”', 'í™”', 'ìˆ˜', 'ëª©', 'ê¸ˆ', 'í† '];
      let timeZoneDate = new Date();
      let offset = timeZoneDate.getTimezoneOffset();
      let centerTimeInfo, operator;

      if (offset !== 0) {

        if (offset < 0) {
          operator = '+';
          offset = offset * -1;
        } else if (offset > 0) {
          operator = '-';
        }
        let hour = offset / 60;
        hour < 10 ? hour = `0${hour}` : hour = hour;
        centerTimeInfo = `${date}T${time}.000${operator}${hour}:00`;

      } else {
        centerTimeInfo = `${date}T${time}.000+00:00`;
      }

      if (dateDetail[1].charAt(0) === '0') dateDetail[1] = dateDetail[1].charAt(1);
      let dayOfWeek = week[new Date(centerTimeInfo).getDay()];
      return `${dateDetail[1]}.${dateDetail[2]}[${dayOfWeek}] ${timeDetail[0]}:${timeDetail[1]}`;
    }

